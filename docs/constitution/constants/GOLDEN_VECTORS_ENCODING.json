    },
    {
      "name": "simple_ascii",
      "input": "hello",
      "expectedBytes": "0000000568656c6c6f",
      "expectedDigest": "2cf24dba4f0a5d5f0a5d5f0a5d5f0a5d5f0a5d5f0a5d5f0a5d5f0a5d5f0a5d",
      "description": "Simple ASCII string"
    },
    {
      "name": "unicode_nfc",
      "input": "caf√©",
